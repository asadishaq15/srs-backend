    "@nestjs/platform-socket.io": "^11.0.7",
    "@nestjs/websockets": "^11.0.7",
    "apollo-server-express": "^3.13.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
